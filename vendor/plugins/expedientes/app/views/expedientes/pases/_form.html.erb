<%= semantic_form_for([:expediente,@pase]) do |form| %>

  <% if @pase.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@pase.errors.count, "error") %> prohibited this person from being saved:</h2>

      <ul>
      <% @pase.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
