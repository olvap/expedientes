<%= table_for(@pases) do |t| %>
  <%= t.data do
    t.cell(:entrada)
    t.cell(:oficina)
    t.cell(:observaciones)
    t.cell(:acciones, :heading => "Acciones", :cell_html => {:class => "acciones"}) do |pase|
      cell_content = link_to 'Detalles', expediente_pase_path(pase.expediente,pase)
      cell_content += link_to 'Editar', edit_expediente_pase_path(pase.expediente,pase)
      cell_content += link_to 'Destroy', expediente_pase_path(pase.expediente,pase), confirm: 'Are you sure?', method: :delete
    end
  end %>
<% end %>
