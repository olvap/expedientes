<h1>Listing oficinas</h1>

<%= paginate @oficinas %>
<%= table_for(@oficinas) do |t| %>
  <%= t.data do
    t.cell(:id)
    t.cell(:name)
    t.cell(:acciones, :heading => "Acciones", :cell_html => {:class => "acciones"}) do |oficina|
      cell_content = link_to 'Detalles', oficina_path(oficina)
      cell_content += link_to 'Editar', edit_oficina_path(oficina)
      cell_content += link_to 'Destroy', oficina_path(oficina), confirm: 'Are you sure?', method: :delete
    end
  end %>
<% end %>

<%= link_to 'New Oficina', new_oficina_path %>
