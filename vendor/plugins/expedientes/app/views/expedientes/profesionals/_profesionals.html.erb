<%= paginate @profesionals %>
<%= table_for(@profesionals) do |t| %>
  <%= t.data do
    t.cell(:id)
    t.cell(:titulo)
    t.cell(:matricula)
    t.cell(:person)
    t.cell(:acciones, :heading => "Acciones", :cell_html => {:class => "acciones"}) do |profesional|
      cell_content = link_to 'Detalles', profesional_path(profesional)
      cell_content += link_to 'Editar', edit_profesional_path(profesional)
      cell_content += link_to 'Destroy', profesional_path(profesional), confirm: 'Are you sure?', method: :delete
    end
  end %>
<% end %>
