<%= paginate @expedientes %>
<%= table_for(@expedientes) do |t| %>
  <%= t.data do
    t.cell(:id)
    t.cell(:numero_expediente_colegio)
    t.cell(:final_de_obra)
    t.cell(:profesionals_format)
    t.cell(:ubicacion_actual)
    t.cell(:fecha_de_entrada)
    t.cell(:partida)
    t.cell(:acciones, :heading => "Acciones", :cell_html => {:class => "acciones"}) do |expediente|
      cell_content = link_to 'Detalles', xpedientes.expediente_path(expediente)
      cell_content += link_to 'Editar', xpedientes.edit_expediente_path(expediente)
      cell_content += link_to 'Destroy', xpedientes.expediente_path(expediente), confirm: 'Are you sure?', method: :delete
      cell_content += link_to 'Mover', xpedientes.new_expediente_pase_path(expediente)
    end
  end %>
<% end %>
