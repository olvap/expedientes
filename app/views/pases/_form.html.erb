<%= semantic_form_for [:admin, :expediente, pase] do |f| %>
  <%= f.semantic_errors %>
  <%= f.inputs "Details" do %>

    <%= f.input :oficina, :collection => expediente.come_from.try(:destinos) || Oficina.iniciales  %>
    <%= f.input :entrada, :as=>:string, :input_html => {:class => 'datepicker',:size=>10} %>
    <%= f.input :observaciones, :input_html => {:class => :ckeditor}, :label => false %>
  <% end %>
  <%= f.buttons %>
<% end %>
